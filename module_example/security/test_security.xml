<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="group_test_admin" model="res.group">
        <field name="name">Test / Admin</field>
    </record>
    <record id="only_active_modify" model="ir.rule">
        <field name="name">Only active Course Can BE Modify</field>
        <field name="perm_read" eval="0"/>
        <field name="perm_write" eval="1"/>
        <field name="perm_create" eval="0"/>
        <field name="perm_unlink" eval="1"/>
        <field name="domain_force">
            [{'active','=',True}]
        </field>
        <field name="model_id" ref="model_test_course"/>
    </record>
</odoo>