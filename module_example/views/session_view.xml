<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <record model="ir.ui.view" id="session_view_form">
            <field name="name">view.session.form</field>
            <field name="model">test.session</field>
            <field name="arch" type="xml">
                <form string="session form">
                    <sheet>
                        <h1>
                            <field name="name"/>
                        </h1>
                        <group>
                            <group>
                                <field name="course_id"/>
                                <field name="Instructor_id"/>
                            </group>
                            <group>
                                <field name="start_date"/>
                                <field name="end_date"/>
                                <field name="duration"/>
                            </group>
                        </group>
                        <notebook>
                            <page string="Students">
                                <field name="student_ids"/>
                            </page>
                        </notebook>
                    </sheet>
                </form>
            </field>
        </record>
        <record model="ir.ui.view" id="session view list">
            <field name="name">view.session.list</field>
            <field name="model">test.session</field>
            <field name="arch" type="xml">
                <tree string="Session List">
                    <field name="name"/>
                    <field name="instructor_id"/>
                </tree>
            </field>
        </record>
        <record model="ir.ui.view" id="session view calendar">
            <field name="name">test.session.calendar</field>
            <field name="model">test.session</field>
            <field name="arch" type="xml">
                <caledar string="Session caledar"
                date_start="start_date"
                date_end="end_date"
                color="instructor_id"
                mode="month">
                    <field name="name"/>
                    <field name="instructor_id"/>
                </caledar>
            </field>
        </record>
        <!--        <record model="ui.ir,view" id="session view search">-->
        <!--            <field name="name">view.session.search</field>-->
        <!--            <field name="model">test.session</field>-->
        <!--            <field name="aech" type="xml">-->
        <!--                <search>-->
        <!--                    <field name="name"/>-->
        <!--                    <field name="description"/>-->
        <!--                    <field name="level"/>-->
        <!--                </search>-->
        <!--            </field>-->
        <!--        </record>-->
    </data>
</odoo>